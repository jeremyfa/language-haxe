    'prefix': 'v'
    'body': '-v\n$0'
  'hxml -debug':
    'prefix': 'debug'
    'body': '-debug\n$0'
  'hxml -dce':
    'prefix': 'dce'
    'body': '-dce ${1:[std|full|no]}\n$0'
  'hxml -swf-version':
    'prefix': 'swf-version'
    'body': '-swf-version ${1:<version>}\n$0'
  'hxml -swf-header':

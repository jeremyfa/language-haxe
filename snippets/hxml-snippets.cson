  'hxml --php-prefix':
    'prefix': 'php-prefix'
    'body': '--php-prefix ${1:<name>}\n$0'
  'hxml --remap':
    'prefix': 'remap'
    'body': '--remap ${1:<package}:${2:target>}\n$0'
  'hxml --interp':
    'prefix': 'interp'
    'body': '--interp\n$0'
  'hxml --macro':
    'prefix': 'macro'
    'body': '--macro ${1:name}\n$0'
  'hxml --eval':
    'prefix': 'eval'
    'body': '--eval ${1:arg}\n$0'
  'hxml --wait':
    'prefix': 'wait'
    'body': '--wait ${2:[127.0.0.1]:}${1:6000}\n$0'
  'hxml --connect':
    'prefix': 'connect'
    'body': '--connect ${1:${3:[host}\:${2:]port}}\n$0'
  'hxml --cwd':
    'prefix': 'cwd'
    'body': '--cwd ${1:dir}\n$0'

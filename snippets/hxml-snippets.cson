    'prefix': 'each'
    'body': '--each\n$0'
  'hxml --display':
    'prefix': 'display'
    'body': '--display\n$0'
  'hxml --no-output':
    'prefix': 'no-output'
    'body': '--no-output\n$0'
  'hxml --times':
    'prefix': 'times'
    'body': '--times\n$0'
  'hxml --no-inline':
    'prefix': 'no-inline'
    'body': '--no-inline\n$0'
  'hxml --no-opt':
    'prefix': 'no-opt'
    'body': '--no-opt\n$0'

'.source.hxml':
  'hxml -cp':
    'prefix': 'cp'
    'body': '-cp ${1:<path>}\n$0'
  'hxml -js':
    'prefix': 'js'
    'body': '-js ${1:<file>}.js\n$0'
  'hxml -swf':
    'prefix': 'swf'
    'body': '-swf ${1:<file>}.swf\n$0'
  'hxml -as3':
    'prefix': 'as3'
    'body': '-as3 ${1:<directory>}\n$0'
  'hxml -neko':
    'prefix': 'neko'
    'body': '-neko ${1:<file>}.n\n$0'
  'hxml -php':
    'prefix': 'php'
    'body': '-php ${1:<directory>}\n$0'
  'hxml -cpp':
    'prefix': 'cpp'
    'body': '-cpp ${1:<directory>}\n$0'
  'hxml -cs':
    'prefix': 'cs'
    'body': '-cs ${1:<directory>}\n$0'
  'hxml -java':
    'prefix': 'java'
    'body': '-java ${1:<directory>}\n$0'
  'hxml -python':
    'prefix': 'python'
    'body': '-python ${1:<file>}\n$0'
  'hxml -xml':
    'prefix': 'xml'
    'body': '-xml ${1:<file>}\n$0'

    'prefix': 'net-lib'
    'body': '-net-lib ${1:${2:<file>[}@${3:std]}}\n$0'
  'hxml -net-std':
    'prefix': 'net-std'
    'body': '-net-std ${1:<file>}\n$0'
  'hxml -c-arg':
    'prefix': 'c-arg'
    'body': '-c-arg ${1:<arg>}\n$0'
  'hxml -x':
    'prefix': 'x'
    'body': '-x ${1:<file>}\n$0'
  'hxml -resource':
    'prefix': 'resource'
    'body': '-resource ${1:${2:<file>[}@${3:name]}}\n$0'
  'hxml -prompt':
    'prefix': 'prompt'
